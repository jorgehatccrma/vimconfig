function! ColorFromHTMLNames(fgname, bgname, ...)
  let fg = get(s:HTMLpalette, a:fgname, ['#FFFFFF', 15])
  let bg = get(s:HTMLpalette, a:bgname, ['#000000', 0])
  let l:color = [fg[0], bg[0], fg[1], bg[1]] + a:000
  " echo l:color
  return l:color
endfunction

function! ColorFromLaTeXNames(fgname, bgname, ...)
  let fg = get(s:LaTeXpalette, a:fgname, ['#FFFFFF', 15])
  let bg = get(s:LaTeXpalette, a:bgname, ['#000000', 0])
  let l:color = [fg[0], bg[0], fg[1], bg[1]] + a:000
  " echo l:color
  return l:color
endfunction

function! ColorFromMaterialNames(fgname, bgname, ...)
  let fg = get(s:MaterialPalette, a:fgname, ['#FFFFFF', 15])
  let bg = get(s:MaterialPalette, a:bgname, ['#000000', 0])
  let l:color = [fg[0], bg[0], fg[1], bg[1]] + a:000
  " echo l:color
  return l:color
endfunction


